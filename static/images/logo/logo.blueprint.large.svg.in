{% extends "logo.base.large.svg" %}

{% block additional_styles %}
	#type {
		fill: #fff;
	}
	#slogan {
		fill: #fff;
	}
	#type-line {
		stroke: #fff;
	}
	#guides {
		stroke: rgba(255, 255, 255, .4);
	}
	#wireframe {
		stroke: #fff;
		stroke-width: 5;
	}
	#bg {
		fill: #2b71b7;
	}
{% endblock %}

{% block logo %}
  {{ draw("guides", guides) }}
  {{ draw("wireframe", wireframe) }}
{% endblock %}

